uci add    ddns  service
uci set    ddns.@service[-1]=service
uci set    ddns.@service[-1].check_interval='5'
uci set    ddns.@service[-1].check_unit='minutes'
uci set    ddns.@service[-1].domain='${f17gw_ddns4_domain}'
